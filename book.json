{
    "title": "开放平台文档",
    "description": "开放平台使用教程",
    "author": "takecloud",
    "language": "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "-highlight",
        "-sharing",
        "prism",
        "search-pro",
        "theme-comscore",
        "tbfed-pagefooter",
        "github",
        "codeblock-filename",
        "expandable-chapters-small"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/takecloud/takecloudapi-sdk-php"
        },
        "tbfed-pagefooter": {
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}